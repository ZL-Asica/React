{
  "entryPoints": [
    // Hooks
    // "src/hooks",
    "src/hooks/async",
    "src/hooks/dom",
    "src/hooks/state",
    // Utils
    // "src/utils",
    "src/utils/array",
    "src/utils/date",
    "src/utils/dom",
    "src/utils/math",
    "src/utils/object",
    "src/utils/string"
  ],
  "out": "docs/docs", // Output directory
  "plugin": ["typedoc-plugin-markdown"], // Use the markdown plugin
  "tsconfig": "tsconfig.typedoc.json", // Use this tsconfig file
  "exclude": ["**/__tests__/**", "node_modules", "dist", "docs/**", "docs/.vitepress/**"], // Exclude these directories
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "hideGenerator": true,
  "readme": "none",
  "categorizeByGroup": true,
  "sort": ["source-order"]
}
